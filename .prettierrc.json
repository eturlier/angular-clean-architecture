{
    "printWidth": 80,
    "tabWidth": 4,
    "useTabs": false,
    "semi":true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "arrowParens": "avoid",
    "endOfLine": "auto",
    "bracketSpacing": true,
    "trailingComma": "es5",
    "htmlWhitespaceSensitivity": "ignore",
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
}
